{
  "name": "ng5-restful",
  "version": "1.3.1",
  "description": "Angular 5+ module for connecting with RESTful API.",
  "author": "Luka",
  "license": "MIT",
  "main": "ng5-restful.umd.js",
  "jsnext:main": "ng5-restful.esm.js",
  "module": "ng5-restful.esm.js",
  "types": "ng5-restful.d.ts",
  "scripts": {
    "test": "echo \"No test specified\" && exit 1",
    "copy": "node tools/copy-package.js && node tools/cleanup-package.js",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc && ngc",
    "postbundle": "node tools/remove-code.js",
    "build": "npm run bundle && npm run copy",
    "publish": "cd ./dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lujo5/ng5-restful.git"
  },
  "keywords": [
    "angular",
    "angular5",
    "rest"
  ],
  "dependencies": {
    "@angular/common": "^5.2.11",
    "@angular/core": "^5.2.11",
    "rxjs": "5.5.11"
  },
  "devDependencies": {
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.11",
    "@types/core-js": "^2.5.0",
    "@types/node": "^10.3.2",
    "del": "^3.0.0",
    "node-sass": "^4.5.3",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.4.2"
  },
  "homepage": "https://github.com/Lujo5/ng5-restful#readme",
  "bugs": {
    "url": "https://github.com/Lujo5/ng5-restful/issues"
  }
}
